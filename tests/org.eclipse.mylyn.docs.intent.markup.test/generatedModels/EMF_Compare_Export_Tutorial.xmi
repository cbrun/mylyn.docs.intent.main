        <content xmi:type="markup:Text" data="&#xA;"/>
        <content xmi:type="markup:Text" data="&#x9;"/>
        <content xmi:type="markup:Text" data="&#xA;"/>
        <content xmi:type="markup:Text" data="&#x9;&#x9;public Image getEnabledImage() {"/>
